{
  "name": "amstragram",
  "version": "1.0.0",
  "description": "Superbe application pour partager tes photos !",
  "engines": {
    "node": "10.x"
  },
  "scripts": {
    "start": "node server",
    "start-dev": "concurrently -n srv,clt -c bgBlue.bold,bgMagenta.bold --handle-input  \"nodemon --ignore react-ui/ server\" \"cd react-ui/ && npm run start\"",
    "front-end-only": "cd react-ui/ && npm run start",
    "heroku-postbuild": "cd react-ui/ && npm install --no-shrinkwrap && npm run build && rm build/static/*/*.map"
  },
  "cacheDirectories": [
    "node_modules",
    "react-ui/node_modules"
  ],
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "cloudinary": "^1.14.0",
    "compression": "^1.7.4",
    "connect-multiparty": "^2.2.0",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "helmet": "^3.15.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.5.2",
    "morgan": "^1.9.1",
    "node-mailjet": "^3.3.1",
    "normalize-port": "^1.0.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Davidshb/amstramgram.git"
  },
  "keywords": [
    "node",
    "heroku",
    "create-react-app",
    "react",
    "instagram"
  ],
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^4.1.0"
  },
  "main": "server/index.js",
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true,
        "experimentalObjectRestSpread": true
      }
    },
    "plugins": [
      "react"
    ]
  }
}
